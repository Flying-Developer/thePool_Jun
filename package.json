{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "cross-env node --require @lukalabs/esbuild-inject-plugin -- ./node_modules/.bin/remix build",
    "deploy": "wrangler publish",
    "dev:remix": "cross-env NODE_ENV=development node --require @lukalabs/esbuild-inject-plugin -- ./node_modules/.bin/remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev": "yarn run build && run-p dev:*",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.7.2",
    "@remix-run/cloudflare-workers": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "axios": "^1.1.2",
    "cross-env": "^7.0.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.2",
    "recoil": "^0.7.5",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@lukalabs/esbuild-inject-plugin": "^1.0.2",
    "@lukalabs/esbuild-styled-components": "^1.0.0",
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/ui": "^0.24.3",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.7.2",
    "happy-dom": "^7.6.0",
    "jest-image-snapshot": "^5.2.0",
    "miniflare": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "wrangler": "^2.0.27"
  },
  "engines": {
    "node": ">=16.13"
  }
}
